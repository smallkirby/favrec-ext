{
  "manifest_version": 2,
  "name": "FavRec",
  "version": "1.0",
  "description": "Record your favorites and remember them forever.",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png"
  },
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png"
    },
    "default_title": "default_title",
    "default_popup": "popup/popup.html"
  }
}
